{
  "compilerOptions": {
    "outDir": "./output/",
    "sourceMap": true,
    "strictNullChecks": false,
    "module": "commonjs",
    "noImplicitAny": false,
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "lib": ["es2017", "dom"],
    "baseUrl": "./src",
    "downlevelIteration": true,
    "paths": {
      "apis/*": ["./API/*"],
      "components/*": ["./components/*"],
      "constant/*": ["./constant/*"]
    }
  },
  "include": ["./src/**/*"],
  "typeRoots": ["./src/types"]
}
